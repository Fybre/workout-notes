{
  "expo": {
    "name": "Workout Notes",
    "slug": "workout-notes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "workoutnotes",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#121212",
      "bundleIdentifier": "com.fybre.workoutnotes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121212"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fybre.workoutnotes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      [
        "expo-speech-recognition",
        {
          "microphonePermission": "Workout Notes uses the microphone for hands-free set logging. For example, tap the mic button and say \"100 by 10\" to log a set of 100kg for 10 reps without typing on the keypad.",
          "speechRecognitionPermission": "Workout Notes uses speech recognition to convert your voice into workout data. For example, saying \"80 by 5\" will be recognized and logged as 80kg for 5 reps.",
          "androidRecognitionService": {
            "packageName": "com.google.android.googlequicksearchbox",
            "action": "android.speech.RecognitionService"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5931dfa0-702d-4397-a5cf-9ac8d953bb39"
      }
    },
    "owner": "fybre"
  }
}
